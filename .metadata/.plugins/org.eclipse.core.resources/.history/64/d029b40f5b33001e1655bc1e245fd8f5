package com.model;

import jakarta.persistence.Entity;

@Entity
public class Regular_employee extends Employee  {
	float salary;
	int bonus;
	public float getSalary() {
		return salary;
	}
	public void setSalary(float salary) {
		this.salary = salary;
	}
	public int getBonus() {
		return bonus;
	}
	public void setBonus(int bonus) {
		this.bonus = bonus;
	}
	
}
